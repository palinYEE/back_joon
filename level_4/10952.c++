#include<iostream>

using namespace std;

int main(){
    int a, b, count = 0;
    int array[10000];
    do
    {
        cin >> a; 
        cin >> b;
        array[count] = a+b;
        count ++;
    } while (a != 0 && b != 0);

    for( int i=0; i< count - 1; i++){
        printf("%d\n",array[i]);
    }
    
    
}